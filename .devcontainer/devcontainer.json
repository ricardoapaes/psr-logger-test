{
	"name": "like/composer-empty",
	"dockerComposeFile": [
		"../docker-compose.yml"
	],
	"service": "php",
  	"shutdownAction": "none",
	"workspaceFolder": "/var/www/public/",
	"customizations": {
		"vscode": {
			"extensions": [
				"bmewburn.vscode-intelephense-client",
				"hbenl.vscode-test-explorer",
				"recca0120.vscode-phpunit",
				"eamodio.gitlens",
				"mhutchie.git-graph",
				"junstyle.php-cs-fixer",
				"janbn.git-last-commit-message"
			]
		}
	}
}